<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Distributed cache implementation of the IServiceTicketStore for Single sign-out</Description>
    <Version>2.1.0</Version>
    <Authors>Galaxy Software Services Corporation</Authors>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageTags>CAS</PackageTags>
    <PackageReleaseNotes>Enable SourceLink</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/akunzai/GSS.Authentication.CAS</PackageProjectUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <Company>Galaxy Software Services Corporation</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/akunzai/GSS.Authentication.CAS/blob/master/LICENSE.md</PackageLicenseUrl>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GSS.Authentication.CAS.Core" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="2.0.0" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-*" PrivateAssets="All" />
  </ItemGroup>
  
</Project>
